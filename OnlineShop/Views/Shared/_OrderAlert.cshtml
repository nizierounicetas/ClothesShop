@if (ViewBag.Message != null)
    {
       <script>Swal.fire({
           title: '@ViewData[WC.MessageAlertName]',
           confirmButtonColor: '#000000',
           icon: 'success',
           position: 'top',
           color: '#000000'
       })</script>
    }
    
@if (ViewBag.ErrorMessage != null)
    {
        <script>Swal.fire({
           title: '@ViewData[WC.ErrorMessageAlertName]',
           confirmButtonColor: '#000000',
           icon: 'warning',
           color: '#000000',
           position: 'top'
       })</script>
    }
